{
  "name": "msgpack-rpc-node",
  "version": "0.1.0-alpha.3",
  "description": "MessagePack RPC implementation for Node",
  "main": "./dist/index.js",
  "scripts": {
    "pretest": "npm run build",
    "test": "jest",
    "test:watch": "jest -w",
    "build": "tsc",
    "format": "prettier --write 'src/**/*.ts'",
    "lint": "eslint --ext .ts src",
    "prepublishOnly": "npm run build && npm test"
  },
  "files": [
    "src/**/*",
    "dist/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/theJian/msgpack-rpc-node.git"
  },
  "keywords": ["msgpack", "rpc", "msgpack-rpc"],
  "author": "theJian <thejianmail@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/theJian/msgpack-rpc-node/issues"
  },
  "homepage": "https://github.com/theJian/msgpack-rpc-node#readme",
  "dependencies": {
    "@msgpack/msgpack": "^1.8.0"
  },
  "devDependencies": {
    "@types/jest": "24.0.19",
    "@types/node": "12.7.12",
    "@typescript-eslint/eslint-plugin": "2.3.1",
    "@typescript-eslint/parser": "2.3.1",
    "eslint": "6.3.0",
    "eslint-config-prettier": "6.3.0",
    "eslint-plugin-prettier": "3.1.1",
    "jest": "24.9.0",
    "prettier": "1.18.2",
    "ts-jest": "24.1.0",
    "typescript": "3.6.4"
  }
}
